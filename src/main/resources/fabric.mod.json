{
  "schemaVersion": 1,
  "id": "ore-simulator",
  "version": "${version}",

  "name": "Ore-Simulator",
  "description": "X-ray on Crack!",
  "authors": [
    "bllry"
  ],
  "contact": {
    "issues": "https://github.com/bllryy/Ore-Simulator/pulls",
    "sources": "https://github.com/bllryy/Ore-Simulator"
  },

  "license": "GPL-3.0",
  "icon": "assets/rejects/METORLOGO1.png",

  "environment": "client",
  "entrypoints": {
    "meteor": [
      "bllry.oresim.bllryAddon"
    ],
    "seedcrackerx": [
      "bllry.oresim.utils.SeedCrackerEP"
    ]
  },
  "mixins": [
    "ore-simulator.mixins.json",
    "ore-simulator-metor.mixins.json"
  ],
  "accessWidener": "Ore-Simulator.accesswidener",
  "custom": {
    "meteor-client:color": "227,0,0",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=21",
    "minecraft": "~${mc_version}",
    "meteor-client": "*"
  }
}
